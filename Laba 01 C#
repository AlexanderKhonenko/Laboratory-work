using System;

public class Program
{
    static double z1,z2;
    static double a;
    public static int Main()
    {
        Console.WriteLine("Введите число");
        a = Console.Read();
        z1 = (Math.Cos(a) + Math.Sin(a)) / (Math.Cos(a) - Math.Sin(a));
        z2 = Math.Tan(2 * a) + 1 /(Math.Cos(2*a));
        Console.WriteLine(z1);
        Console.WriteLine(z2);
        return 0;
    }
}
